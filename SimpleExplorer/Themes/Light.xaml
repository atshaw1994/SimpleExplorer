<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--#region Global-->

    <SolidColorBrush x:Key="Shade1" Color="#20000000"/>
    <SolidColorBrush x:Key="Shade1.Border" Color="#60000000"/>
    <SolidColorBrush x:Key="Shade2" Color="#40000000"/>
    <SolidColorBrush x:Key="Shade2.Border" Color="#80000000"/>
    <SolidColorBrush x:Key="Global.Background" Color="#fff"/>
    <SolidColorBrush x:Key="Global.Foreground" Color="#000"/>
    <SolidColorBrush x:Key="Global.Foreground.Shade1" Color="#808080"/>
    <SolidColorBrush x:Key="Global.Glyph.Stroke" Color="#000"/>
    <Color x:Key="Global.Glyph.Stroke.Color">#000</Color>
    
    <!--#endregion-->
    
    <!--#region CaptionBar-->

    <SolidColorBrush x:Key="CaptionBar.Background.Active" Color="#fff"/>
    <SolidColorBrush x:Key="CaptionBar.Foreground.Active" Color="#000"/>
    <SolidColorBrush x:Key="MouseOverOverlayBackgroundBrush" Color="#33000000" />
    <SolidColorBrush x:Key="PressedOverlayBackgroundBrush" Color="#44000000" />
    <SolidColorBrush x:Key="MouseOverWindowCloseButtonBackgroundBrush" Color="#e81123" />
    <SolidColorBrush x:Key="PressedWindowCloseButtonBackgroundBrush" Color="#f1707a" />
    <SolidColorBrush x:Key="WindowMainPanel.Background" Color="#000"/>
    <SolidColorBrush x:Key="WindowMainPanel.Border" Color="#7D000000"/>

    <!--#endregion-->

    <!--#region NavButton-->

    <SolidColorBrush x:Key="NavButton.Static.Background" Color="#80ffffff"/>
    <SolidColorBrush x:Key="NavButton.Foreground" Color="#000"/>
    <SolidColorBrush x:Key="NavButton.Static.Border" Color="#000"/>

    <SolidColorBrush x:Key="NavButton.MouseOver.Background" Color="#33000000"/>
    <SolidColorBrush x:Key="NavButton.MouseOver.Border" Color="#33000000"/>

    <SolidColorBrush x:Key="NavButton.Pressed.Background" Color="#44000000"/>
    <SolidColorBrush x:Key="NavButton.Pressed.Border" Color="#44000000"/>

    <SolidColorBrush x:Key="NavButton.Disabled.Background" Color="#FFF4F4F4"/>
    <SolidColorBrush x:Key="NavButton.Disabled.Border" Color="#FFADB2B5"/>
    <SolidColorBrush x:Key="NavButton.Disabled.Foreground" Color="#FF838383"/>

    <!--#endregion-->

    <!--#region AddressBar-->

    <SolidColorBrush x:Key="TextBox.Static.Background" Color="#80ffffff"/>
    <SolidColorBrush x:Key="TextBox.Static.Border" Color="#000"/>
    <SolidColorBrush x:Key="TextBox.MouseOver.Border" Color="#40000000"/>
    <SolidColorBrush x:Key="TextBox.Focus.Border" Color="#80000000"/>

    <!--#endregion-->

    <!--#region UI Controls-->

    <SolidColorBrush x:Key="UIElement.Static.Glyph" Color="#b8b8b8"/>
    <SolidColorBrush x:Key="UIElement.MouseOver.Glyph" Color="#60000000"/>

    <!--#endregion-->

    <!--#region Scrollbar-->

    <SolidColorBrush x:Key="Scrollbar.Background" Color="#fff"/>
    <Color x:Key="Scrollbar.MouseOver">#dadada</Color>
    <Color x:Key="Scrollbar.Pressed">#606060</Color>
    <SolidColorBrush x:Key="Scrollbar.Thumb.Background" Color="#000"/>

    <!--#endregion-->

    <!--#region ProgressBar-->

    <SolidColorBrush x:Key="ProgressBar.Track" Color="#656565"/>
    <SolidColorBrush x:Key="ProgressBar.Fill" Color="#656565"/>

    <!--#endregion-->
</ResourceDictionary>